<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <title>Student Data</title>
</head>

<body>

    <h1>This is Student Show Data</h1>

    <table border="1">
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Role</th>
            <th>Technology</th>
        </tr>

        <% employees.forEach(item=> { %>
            <tr>
                <td>
                    <%= item.FirstName %>
                </td>
                <td>
                    <%= item.LastName %>
                </td>
                <td>
                    <%= item.role %>
                </td>
                <td>
                    <%= item.technology %>
                </td>
            </tr>
            <% }) %>

    </table>

</body>

</html>