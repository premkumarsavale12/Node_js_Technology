<!DOCTYPE html>
<html>

<head>
    <title>MongoDB Data Display</title>
</head>

<body>

    <table border="1">
        <tr>
            <th>FirstName</th>
            <th>LastName</th>
            <th>City</th>
            <th>Age</th>
            <th>Role</th>
            <th>Operation</th>
        </tr>

        <%- data.map(item=> `
            <tr>
                <td>${item.FirstName}</td>
                <td>${item.LastName}</td>
                <td>${item.City}</td>
                <td>${item.age}</td>
                <td>${item.role}</td>
                <td><a href="/ui/delete/${item._id}"> delete</td>
            </tr>
            `).join("") %>

    </table>

</body>

</html>